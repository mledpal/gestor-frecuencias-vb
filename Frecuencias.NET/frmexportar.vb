				MsgBox("Debe Seleccionar alguna opción para ejecutar esta opcion", MsgBoxStyle.Critical)
				Exit Sub
			End If
		End If
		
		MsgBox("Archivo Creado Con Éxito", MsgBoxStyle.Information)
		Me.Close()
		frmFrecuencias.Show()
	End Sub
	
